<document name="Excel 操作测试">
    <describe name="Excel 对象">
        <it name="实例化一个 Excel 对象">
            <set name="path" resource="data/excel/demo.xlsx"/>
            <instance name="excel" class="excel">
                <param value="${path}"/>
            </instance>
        </it>
        <it name="获取 Excel 多个工作表对象">
            <set name="path" resource="data/excel/demo.xlsx"/>
            <instance name="excel" class="excel">
                <param value="${path}"/>
            </instance>
            <set name="sheets" target="excel" property="sheets"/>
            <out value="${sheets}"/>
        </it>
    </describe>
</document>