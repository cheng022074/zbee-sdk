<document name="对象函数">
    <describe name="object.setOnce">
        <it name="两次设置的值必须相等">
            <set name="target" value="${{}}"/>
            <call-function var="value1" name="object.setOnce">
                <param value="${target}"/>
                <param value="name"/>
                <param value="${() => new Map()}"/>
            </call-function>
            <call-function var="value2" name="object.setOnce">
                <param value="${target}"/>
                <param value="name"/>
                <param value="${() => new Map()}"/>
            </call-function>
            <assert-not-equal actual="${value1}" expected="${undefined}"/>
            <assert-not-equal actual="${value2}" expected="${undefined}"/>
            <assert-equal actual="${value1}" expected="${value2}"/>
        </it>
    </describe>
</document>