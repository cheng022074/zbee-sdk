<describe name="校验函数">
    <describe name="is.string">
        <it name="给定字符串数据返回true">
            <assert-function name="is.string" value="${true}">
                <param value="字符串"/>
            </assert-function>
        </it>
        <it name="给定数值数据返回false">
            <assert-function name="is.string" value="${false}">
                <param value="${100}"/>
            </assert-function>
        </it>
    </describe>
    <describe name="is.number">
        <it name="给定数值数据返回true">
            <assert-function name="is.number" value="${true}">
                <param value="${100}"/>
            </assert-function>
        </it>
        <it name="给定字符串数据返回false">
            <assert-function name="is.number" value="${false}">
                <param value="字符串"/>
            </assert-function>
        </it>
    </describe>
    <describe name="is.boolean">
        <it name="给定布尔数据返回true">
            <assert-function name="is.boolean" value="${true}">
                <param value="${false}"/>
            </assert-function>
        </it>
        <it name="给定字符串数据返回false">
            <assert-function name="is.boolean" value="${false}">
                <param value="字符串"/>
            </assert-function>
        </it>
    </describe>
    <describe name="is.array">
        <it name="给定数组数据返回true">
            <assert-function name="is.array" value="${true}">
                <param value="${[1,2,3]}"/>
            </assert-function>
        </it>
        <it name="给定字符串数据返回true">
            <assert-function name="is.array" value="${false}">
                <param value="字符串"/>
            </assert-function>
        </it>
    </describe>
    <describe name="is.object">
        <it name="给定对象数据返回true">
            <assert-function name="is.object" value="${true}">
                <param value="${{}}"/>
            </assert-function>
        </it>
        <it name="给定实例数据返回true">
            <assert-function name="is.object" value="${true}">
                <param value="${new class{}}"/>
            </assert-function>
        </it>
    </describe>
     <describe name="is.object.simple">
        <it name="给定对象数据返回true">
            <assert-function name="is.object.simple" value="${true}">
                <param value="${{}}"/>
            </assert-function>
        </it>
        <it name="给定实例数据返回false">
            <assert-function name="is.object.simple" value="${false}">
                <param value="${new class{}}"/>
            </assert-function>
        </it>
    </describe>
    <describe name="is.primitive">
        <it name="给定原始数据返回true">
            <assert-function name="is.primitive" value="${true}">
                <param value="字符串"/>
            </assert-function>
        </it>
        <it name="给定数组数据返回false">
            <assert-function name="is.primitive" value="${false}">
                <param value="${[]}"/>
            </assert-function>
        </it>
    </describe>
    <describe name="is.date">
        <it name="给定日期数据返回true">
            <assert-function name="is.date" value="${true}">
                <param value="${new Date()}"/>
            </assert-function>
        </it>
        <it name="给定日期毫秒数数据返回false">
            <assert-function name="is.date" value="${false}">
                <param value="${Date.now()}"/>
            </assert-function>
        </it>
    </describe>
    <describe name="is.defined">
        <it name="给定 NULL 数据返回true">
            <assert-function name="is.defined" value="${true}">
                <param value="${null}"/>
            </assert-function>
        </it>
        <it name="给定 UNDEFINED 数据返回false">
            <assert-function name="is.defined" value="${false}">
                <param value="${undefined}"/>
            </assert-function>
        </it>
    </describe>
    <describe name="is.empty">
        <it name="给定空字符串返回true">
            <assert-function name="is.empty" value="${true}">
                <param value="${''}"/>
            </assert-function>
        </it>
        <it name="给定数字返回false">
            <assert-function name="is.empty" value="${false}">
                <param value="${100}"/>
            </assert-function>
        </it>
    </describe>
</describe>