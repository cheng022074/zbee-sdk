<class description="Excel 中单个工作表对象">
    <constructor description="使用单个工作表对象构建" implement="excel.sheet.constructor">
        <param name="sheet" type="Sheet" description="工作表原生对象"/>
    </constructor>
    <methods>
        <method description="获取所有标题信息" name="getTitles" implement="excel.sheet.methods.getTitles" type="array">
            <param name="address" type="string" description="单元格区域信息"/>
        </method>
        <method description="获取所有数据信息" name="getRecords" implement="excel.sheet.methods.getRecords" type="array">
            <param name="address" type="string" description="单元格区域信息"/>
        </method>
    </methods>
</class>