{
    "extend":".tree.base",
    "mixins":[
        "mixin.region"
    ],
    "class":true,
    "constructor":false,
    "properties":{
        "region":{
            "get":".tree.region"
        },
        "depth":{
            "get":".tree.depth"
        },
        "childCountScopeHeight":{
            "get":".tree.height.scope.child"
        },
        "scopeHeight":{
            "get":".tree.height.scope"
        },
        "descendantNodes":{
            "get":".tree.nodes.descendant"
        },
        "children":{
            "get":".tree.nodes.child"
        },
        "lastLeafNode":{
            "get":".tree.node.leaf.last"
        },
        "leafNodes":{
            "get":".tree.nodes.leaf"
        }
    },
    "methods":[
        "layout",
        "expand",
        "collapse",
        "select",
        "deselect",
        {
            "name":"selectUp",
            "impl":".tree.select.up"
        },{
            "name":"selectDown",
            "impl":".tree.select.down"
        },{
            "name":"selectLeft",
            "impl":".tree.select.left"
        },{
            "name":"selectRight",
            "impl":".tree.select.right"
        },{
            "name":"getDepthNodes",
            "impl":".tree.nodes.depth"
        },{
            "name":"appendChild",
            "impl":".tree.append"
        },{
            "name":"insertAfter",
            "impl":".tree.insert.after"
        },{
            "name":"removeChild",
            "impl":".tree.remove"
        }
    ]
}