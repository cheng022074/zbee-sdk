{
    "extend":".tree.base",
    "mixins":[
        "mixin.region"
    ],
    "class":true,
    "constructor":false,
    "staticProperties":{
        "fieldConfigurations":{
            "get":".tree.configurations"
        }  
    },
    "properties":{
        "selected":{
            "get":".tree.node.selected"
        },
        "previousSiblingNode":{
            "get":".tree.node.sibling.previous"
        },
        "nextSiblingNode":{
            "get":".tree.node.sibling.next"
        },
        "firstChildNode":{
            "get":".tree.node.first"
        },
        "lastChildNode":{
            "get":".tree.node.last"
        },
        "expanded":{
            "get":".tree.expanded"
        },
        "synchronized":{
            "get":".tree.nodes.synchronized"
        },
        "isRoot":{
            "get":".tree.is.root"
        },
        "isLeaf":{
            "get":".tree.is.leaf"
        },
        "hidden":{
            "get":".tree.hidden"
        },
        "region":{
            "get":".tree.region"
        },
        "depth":{
            "get":".tree.depth"
        },
        "childCountScopeHeight":{
            "get":".tree.height.scope.child"
        },
        "scopeHeight":{
            "get":".tree.height.scope"
        },
        "descendantNodes":{
            "get":".tree.nodes.descendant"
        },
        "children":{
            "get":".tree.nodes.child"
        },
        "lastLeafNode":{
            "get":".tree.node.leaf.last"
        },
        "leafNodes":{
            "get":".tree.nodes.leaf"
        }
    },
    "methods":[
        "show",
        "hide",
        "layout",
        "expand",
        "collapse",
        "select",
        "deselect",
        {
            "name":"synchronize",
            "impl":".tree.nodes.synchronize"
        },
        {
            "name":"selectUp",
            "impl":".tree.select.up"
        },{
            "name":"selectDown",
            "impl":".tree.select.down"
        },{
            "name":"selectLeft",
            "impl":".tree.select.left"
        },{
            "name":"selectRight",
            "impl":".tree.select.right"
        },{
            "name":"getDepthNodes",
            "impl":".tree.nodes.depth"
        },{
            "name":"appendChild",
            "impl":".tree.append"
        },{
            "name":"insertAfter",
            "impl":".tree.insert.after"
        },{
            "name":"removeChild",
            "impl":".tree.remove"
        },
        "contains"
    ]
}